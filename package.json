{
  "name": "@tb-dev/eslint-config",
  "version": "6.7.1",
  "description": "ESLint config",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.6.5",
  "homepage": "https://github.com/ferreira-tb/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "svelte",
    "vue"
  ],
  "scripts": {
    "build": "vite build",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "=19.0.0-beta-3229e95-20250315",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-svelte": "^3.3.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "svelte": "^5.25.2",
    "svelte-eslint-parser": "^1.1.0",
    "vue-eslint-parser": "^10.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "eslint": "^9.22.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
