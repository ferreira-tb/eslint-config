{
  "name": "@tb-dev/eslint-config",
  "version": "1.6.9",
  "description": "ESLint config",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@8.15.0",
  "homepage": "https://github.com/ferreira-tb/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/eslint-config/issues"
  },
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
  },
  "scripts": {
    "build": "vite build",
    "bump": "miho bump -k --add .",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky install",
    "release": "pnpm run build && pnpm run bump && pnpm publish",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.20.1",
    "globals": "^13.24.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20.11.10",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
