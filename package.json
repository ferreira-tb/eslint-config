{
  "name": "@tb-dev/eslint-config",
  "version": "8.2.0",
  "description": "ESLint config",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.24.0",
  "funding": "https://github.com/sponsors/ferreira-tb",
  "homepage": "https://github.com/ferreira-tb/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "vue"
  ],
  "scripts": {
    "build": "vite build",
    "format": "dprint fmt",
    "lint": "eslint . --config eslint.config.js",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
