{
  "name": "@tb-dev/eslint-config",
  "version": "6.3.0",
  "description": "ESLint config",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.4.1",
  "homepage": "https://github.com/ferreira-tb/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "svelte",
    "vue"
  ],
  "scripts": {
    "build": "vite build",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e552027-20250112",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-svelte": "^3.0.0-next.18",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "svelte": "^5.20.1",
    "svelte-eslint-parser": "^1.0.0-next.13",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "prettier": "^3.5.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "peerDependencies": {
    "eslint": "^9.20.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
